generator client {
  provider             = "prisma-client-py"
  interface            = "asyncio"
  recursive_type_depth = "5"
}

datasource db {
  provider = "sqlite"
  url      = "file:banners.db"
}

model banners {
  id       Int     @id @default(autoincrement())
  content  String?
  markedUp String?
}
